try:
    from PIDcontroller import main
except ImportError:
    import os, sys
    __this_dir = os.path.dirname(os.path.abspath(__file__))
    __package_dir = os.path.join(__this_dir, os.path.pardir)
    sys.path.append(__package_dir)
    from PIDcontroller import main

if __name__ == "__main__":
    main()
